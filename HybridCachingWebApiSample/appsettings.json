{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "EasyCaching": "Debug", // Enable detailed logging for EasyCaching
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "HybridCacheOptions": {
    "RedisHost": "localhost",
    "RedisPort": 6379,
    "RedisInstanceName": "redisCache",
